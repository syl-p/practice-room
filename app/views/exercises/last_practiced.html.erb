<%= turbo_frame_tag "last_practiced" do %>
  <ul role="list" class="gap-x-4 gap-y-4 lg:grid xl:grid-cols-3 lg:grid-flow-row-dense">
    <% @last_practiced.each do |exercise| %>
      <li class="">
        <%= render partial: "exercises/card", locals: {exercise: exercise} %>
      </li>
    <% end %>
  </ul>
<% end %>
