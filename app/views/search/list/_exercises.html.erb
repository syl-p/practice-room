<h2 class="bg-gray-100 px-4 py-2.5 text-xs font-semibold text-gray-900"><%= key %></h2>
<ul class="mt-2 text-sm text-gray-800">
  <% results[key].each do |result| %>
    <li class="cursor-default select-none px-4 py-2" id="option-1" role="option" tabindex="-1">
      <%= link_to result.title, exercise_path(result.id), data: {turbo: false}  %>
    </li>
  <% end %>
</ul>
