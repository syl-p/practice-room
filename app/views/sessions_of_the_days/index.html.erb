<h1>Sessions Of The Days</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions_of_the_days.each do |sessions_of_the_day| %>
      <tr>
        <td><%= link_to 'Show', sessions_of_the_day %></td>
        <td><%= link_to 'Edit', edit_sessions_of_the_day_path(sessions_of_the_day) %></td>
        <td><%= link_to 'Destroy', sessions_of_the_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

